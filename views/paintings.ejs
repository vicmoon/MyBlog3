<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Paintings Gallery</title>
    <link rel="stylesheet" href="/css/paintings.css" />
  </head>
  <body>
    <a href="/" class="back-button"><span>Back</span></a>
    <div class="carousel">
      <% paintings.forEach(function(painting) { %>
      <div class="carousel-item">
        <img src="<%= painting.paintingImage %>" alt="<%= painting.title %>" />
      </div>
      <% }); %>
    </div>
    <a href="/" class="back-button"><span>Back</span></a>
  </body>
</html>
